{
  "name": "reacttstemplate",
  "version": "1.0.0",
  "description": "React, TypeScript, webpack、Babel, EsLint, Prettierあたりのボイラープレートを考えてた。",
  "scripts": {
    "build": "webpack --env.NODE_ENV=production && cp ./public/index.html ./build",
    "dev": "webpack-dev-server --env.NODE_ENV=development",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "format": "prettier --ignore-path .eslintignore --write \"**/*.+(js|jsx|json|ts|tsx)\"",
    "check:type": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shibayaman/reactTSTemplate.git"
  },
  "author": "shibayaman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shibayaman/reactTSTemplate/issues"
  },
  "homepage": "https://github.com/shibayaman/reactTSTemplate#readme",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-react": "^7.19.0",
    "prettier": "1.19.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  }
}
